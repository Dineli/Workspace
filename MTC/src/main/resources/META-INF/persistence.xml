    <class>com.nus.mtc.entity.Locations</class>
    <class>com.nus.mtc.entity.Studys</class>
    <class>com.nus.mtc.entity.Countrys</class>
    <class>com.nus.mtc.entity.Samples</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
